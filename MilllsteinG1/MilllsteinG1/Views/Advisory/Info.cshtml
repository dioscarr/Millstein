@model MilllsteinG1.Models.TeamModel
@{
    ViewBag.Title = "Info";
    Layout = "~/Views/Shared/_Secomdary_Layout.cshtml";

    ViewBag.AdvisoryActive = "menu_top_border";
}

<div id="returndata">
    @Html.Partial("LoadTeamMember")
</div>

@section scripts{
    <script>
        $(function () {
            
            $('.members').click( function (event) {
            $.ajax({
                type:"Get",
                url: "/advisory/LoadTeamMember/" + $(this).attr("id"),
                success: function (data) {
                    $('#returndata').html(data);
                }   
            });
            event.preventDefault();

            });
        });
    </script>
    }