@model MilllsteinG1.Models.ContactModel
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Secomdary_Layout.cshtml";

    ViewBag.ContactActive = "menu_top_border";
    ViewBag.Firmfooter = "on";
}



<div class="row firm_title">
    <div class="col-xs-9 logo">
        <img src="~/Images/logo-all.png" />
    </div>
    <div class="col-xs-3 firm_sidebar">
        <ul class="list-inline firm_sidebar_ul">
            <li><a href="#">Contact</a></li>
        </ul>
    </div>
</div>
<div class="row contact_content">
    <div class="col-xs-9">
        <div class="contact_information">
            <h3>@Model.ContactDetail.ContentHeader</h3>
            <span>@Model.ContactDetail.FirstName @Model.ContactDetail.LastName Main</span><br />
            <span><a class="news_archive_link" href="#">@Model.ContactDetail.Email</a></span><br />
            <span>@Model.ContactDetail.Tel</span><br /><br />
            <h3>@Model.ContactDetail.CareersHeader</h3>
            <span><a class="news_archive_link" href="#">@Model.ContactDetail.CareersEmail</a></span>
        </div>
    </div>
    <div class="col-xs-3">
        <ul class="left_bar_advisory">
          
                <li><a href="~/Contact/NyCity/@Model.Locations[0].Id">@Model.Locations[0].LocationName</a></li>
            <li><a href="~/Contact/WashingtonDCCity/@Model.Locations[1].Id">@Model.Locations[1].LocationName</a></li>
                
           
            
        </ul>
    </div>
</div>
